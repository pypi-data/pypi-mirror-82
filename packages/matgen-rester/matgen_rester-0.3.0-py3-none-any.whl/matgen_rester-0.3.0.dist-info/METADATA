Usage
import matgen_rester as mr
//filter by volume, spaceGroup,crystalSystem
with mr.MatgenRester() as m:
   kwargs = {
     "elements":"Fe,O",
     "volume":[0,100],
     "spaceGroup":[""],
     "crystalSystem":[""],
     "fileds":["cisdid","formula"]
   }
   list = q.get_dft_filter(**kwargs)
 //get by icsdid
 with mr.MatgenRester() as m:
   data = m.get_dft_by_icsdid(icsdid=16887,fields=None)

